  "icons": [
    {
      "src": "./icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "./icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  
  "categories": ["education", "lifestyle", "productivity"],
  
  "prefer_related_applications": false,
  
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  
  "file_handlers": [
    {
      "action": "./",
      "accept": {
        "application/json": [".json"]
      }
    }
